service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
    match /domains/{domain} {
    	allow read: if true;
    }
    match /domains/{domain}/responses/{response} {
    	allow create: if true;
      allow update, read: if false;
    }
  }
}